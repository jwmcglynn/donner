  "editor.tabSize": 2,
  // C++ extension settings
  "C_Cpp.autocompleteAddParentheses": true,
  "C_Cpp.default.includePath": [
    ".",
  ],
  "C_Cpp.clang_format_sortIncludes": true,
  "C_Cpp.intelliSenseEngineFallback": "enabled",
  // Code formatting
  "editor.formatOnSave": true,
  "bazel.buildifierFixOnFormat": true,
  // LLDB
  "lldb.launch.initCommands": [
    "command source ${workspaceRoot}/.lldbinit"
  ],
  // Tests
  "testMate.cpp.test.advancedExecutables": [
    {
      "pattern": "bazel-bin/base/base_tests",
      "runTask": {
        "before": [
          "base_tests"
        ],
      }
    },
    {
      "pattern": "bazel-bin/base/parser/parser_tests",
      "runTask": {
        "before": [
          "base_parser_tests"
        ],
      }
    },
    {
      "pattern": "bazel-bin/svg/core/core_tests",
      "runTask": {
        "before": [
          "core_tests"
        ],
      }
    },
    {
      "pattern": "bazel-bin/svg/parser/parser_tests",
      "runTask": {
        "before": [
          "svg_parser_tests"
        ],
      }
    },
    {
      "pattern": "bazel-bin/svg/components/components_tests",
      "runTask": {
        "before": [
          "components_tests"
        ],
      }
    },
    {
      "pattern": "bazel-bin/svg/properties/tests/properties_tests",
      "runTask": {
        "before": [
          "properties_tests"
        ],
      }
    },
    {
      "pattern": "bazel-bin/svg/tests/svg_tests",
      "runTask": {
        "before": [
          "svg_tests"
        ],
      }
    },
    {
      "pattern": "bazel-bin/svg/xml/xml_tests",
      "runTask": {
        "before": [
          "xml_tests"
        ],
      }
    },
    {
      "pattern": "bazel-bin/svg/xml/xml_details_tests",
      "runTask": {
        "before": [
          "xml_details_tests"
        ],
      }
    },
    {
      "pattern": "bazel-bin/css/parser/parser_tests",
      "runTask": {
        "before": [
          "css_parser_tests"
        ],
      }
    }
  ],
