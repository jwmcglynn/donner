custom_data_source: {
  name: "donner_package"
  base_data_source: "compileunits"

  rewrite: {
    pattern: "^(\\.\\./)?src"
    replacement: "src"
  }
  rewrite: {
    pattern: "^(\\.\\./)?(third_party/\\w+)"
    replacement: "\\2"
  }
  rewrite: {
    pattern: "^(\\.\\./)?external/(\\w+)"
    replacement: "third_party/\\2"
  }
}
